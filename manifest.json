{
   "background": {
      "persistent": false,
      "scripts": [ "extra/jquery-1.11.2.min.js", "extra/parse-1.3.4.min.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "img/icons/16.png",
      "default_popup": "popup.html",
      "default_title": "Adloid"
   },
   "content_scripts": [ {
      "css": [ "extra/notify.css" ],
      "js": [ "extra/jquery-1.11.2.min.js","extra/notify-combined.js"],
      "matches": [ "http://*/*", "https://*/*" ]
   }, {
      "css": [ "extra/notify.css","foodpanda/css/foodpanda.css" ],
      "js": [ "extra/jquery-1.11.2.min.js","extra/notify-combined.js", "foodpanda/js/foodpanda-checkout.js" ],
      "matches": [ "https://www.foodpanda.in/checkout/*" ]
   }, {
      "css": [ "extra/notify.css", "foodpanda/css/foodpanda.css" ],
      "js": [ "extra/jquery-1.11.2.min.js", "extra/notify-combined.js", "foodpanda/js/foodpanda-reviewOrder.js" ],
      "matches": [ "https://www.foodpanda.in/review-order/*" ]
   }, {
      "css": [ "extra/notify.css" ],
      "js": [ "extra/jquery-1.11.2.min.js", "extra/notify-combined.js", "foodpanda/js/foodpandaGetArea.js" ],
      "matches": [ "https://www.foodpanda.in/restaurant/*" ]
   } ],
   "description": "Use Adloid to save both Time & Money!!",
   "icons": {
      "128": "img/icons/128.png",
      "16": "img/icons/16.png",
      "36": "img/icons/36.png",
      "48": "img/icons/48.png",
      "96": "img/icons/96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Adloid ",
   "permissions": [ "notifications","activeTab","storage","tabs" ],
   "short_name": "Adloid",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.3",
   "web_accessible_resources": [  ]
}
